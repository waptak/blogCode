 <div class="mobile-menu">      

      <% if ( theme.favicon){ %>
      <img class="slide-icon" src= <%- theme.favicon %> >   
      <% } %>

      <% for (var i in theme.menu){ %>        
            <a class="mobile-menu-link"  href="<%- url_for(theme.menu[i]) %>"><%= __('Menu.'+i) %>
            </a>
      <% } %>   


      <% theme.widgets.forEach (function (widget) {%>
          <% if( widget == 'search' ){%>
            <a class="mobile-menu-link mobile-menu-search"><%=__('Search.Sidebar_words')%> </a>
          <% } %>
      <% }) %>
</div>