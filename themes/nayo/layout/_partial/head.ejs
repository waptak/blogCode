<head>
  <meta http-equiv="content-type" content="text/html; charset=utf-8">
  <meta name="viewport"content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=yes"/>
  <%
  var title = titlecase(page.title);

  if (is_archive()){
    title = __('Page.Archives');   
  } else if (is_category()){
    title = __('Page.Categories') + ': ' + page.category;
  } else if (is_tag()){
    title = __('Page.Tags') + ': ' + page.tag;
  }
  %>
  
  <title><% if (title){ %>  <%= title %> | <% } %>  <%= theme.title %></title>

 
  <% if (theme.favicon){ %>
    <link rel="icon" href="<%- theme.favicon %>">
  <% } %>


  <!-- css -->
  <%- css('css/style.css') %>  
  <!-- Jquery -->
  <%- js('//cdn.staticfile.org/jquery/3.2.1/jquery.min.js') %> 
  <!-- Add fancyBox -->
  <%- css('https://cdnjs.cloudflare.com/ajax/libs/fancybox/3.1.25/jquery.fancybox.min.css') %>
  <%- js('https://cdnjs.cloudflare.com/ajax/libs/fancybox/3.1.25/jquery.fancybox.min.js') %> 
  <%- js('js/gallery.js') %>
  <!-- javascript --> 
  <%- js('js/mobile.js') %>  
  <%- js('js/utils.js') %>    
  <%- js('js/script.js') %>   
    
  

</head>